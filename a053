#include <iostream>

using namespace std;

int main()
{
    int n; // 題目輸入一個解題量 n

    while(cin >> n) //迴圈輸入解題量
    {
        if (n <= 10) //如果解題量少於等於10題 一題6分 得分公式 n*6
        {
            cout << n*6 << endl; // 故得分公式 n*6
        }

        else if (n <= 20) //如果解題量在11~20題 基本分(前10題得分)60外 後來每題2分
        {
            cout << 2*n + 40 << endl; //如上述條件 得分公式為 60 + (n-10)*2 化簡為 2*n + 40
        }

        else if (n <= 40) //如果解題量在21~40題 基本分(前20題得分共 80分外)外 後來每題1分 (亦可由 60 + (n-10)*2 + (n-20)*1 推導)
        {
            cout << n+60 << endl; //如上述條件 得分公式為 80 + (n-20)*1 化簡為 n + 60
        }

        else if (n > 40) //如果解題量多於40題 一律100分
        {
            cout << 100 << endl; // 100分!
        }

    }

return 0;
}
